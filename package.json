{
  "name": "imukaneza",
  "version": "1.0.0",
  "description": "imukaneza",
  "main": "app.js",
  "scripts": {
    "heroku-postbuild": "babel src -d build",
    "migrate": "npx sequelize-cli db:migrate",
    "start": "node_modules/cross-env/src/bin/cross-env.js NODE_ENV=production npm run migrate && node ./build/app.js",
    "dev": "nodemon --exec babel-node ./src/app.js",
    "createModel": "npx sequelize-cli model:generate ",
    "migrate-undo": " npx sequelize-cli db:migrate:undo",
    "migrate-undo-all": " npx sequelize-cli db:migrate:undo:all",
    "createSeed": "npx sequelize-cli seed:generate",
    "seed": "npx sequelize-cli db:seed:all",
    "seed-undo": "npx sequelize-cli db:seed:undo",
    "seed-undo-all": "npx sequelize-cli db:seed:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Theoneste1/imukaneza.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Theoneste1/imukaneza/issues"
  },
  "homepage": "https://github.com/Theoneste1/imukaneza#readme",
  "dependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@sendgrid/mail": "^7.4.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^9.0.0",
    "express": "^4.17.1",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.6.2",
    "uuid": "^8.3.2",
    "uuidv4": "^6.2.8"
  },
  "devDependencies": {
    "@babel/node": "^7.13.13",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.1",
    "@babel/runtime": "^7.14.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.7",
    "sequelize-cli": "^6.2.0"
  }
